<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.shiftList.ShiftList">
   <children>
      <TableView fx:id="table" layoutX="30.0" layoutY="68.0" prefHeight="200.0" prefWidth="520.0">
        <columns>
          <TableColumn fx:id="shiftID" prefWidth="47.0" text="ID" />
          <TableColumn fx:id="managerID" prefWidth="77.0" text="Manager ID" />
            <TableColumn fx:id="workerID" prefWidth="72.0" text="Worker ID" />
            <TableColumn fx:id="dateID" prefWidth="122.0" text="Date" />
            <TableColumn fx:id="descriptionID" prefWidth="201.0" text="Description" />
        </columns>
      </TableView>
      <Button layoutX="36.0" layoutY="304.0" mnemonicParsing="false" onAction="#onDeleteShift" text="Delete shift" />
      <Button layoutX="152.0" layoutY="304.0" mnemonicParsing="false" onAction="#onBack" text="Back" />
   </children>
</AnchorPane>
